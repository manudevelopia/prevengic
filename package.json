  "license": "MIT",
  "scripts": {
    "build": "webpack",
    "watch": "webpack --watch -d",
    "postinstall": "webpack -p --progress",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "test:coverage": "jest --verbose --coverage"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/test/js/test-shim.js",
      "<rootDir>/src/test/js/test-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/src/test/js/test-preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "@types/react": "^16.7.11",
    "@types/react-dom": "^16.0.11",
    "@types/reactstrap": "^6.4.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "reactstrap": "^6.5.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.10",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "jest": "^23.6.0",
    "react-test-renderer": "^16.6.3",
    "source-map-loader": "^0.2.4",
    "typescript": "^3.2.1",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2"
  }
